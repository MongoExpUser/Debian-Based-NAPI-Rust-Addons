{
  "name": "napi-rust-project",
  "version": "2022.07.11",
  "description": "Rust Container Project for NAPI-Rust",
  "main": "./index.js",
  "license": "MIT",
  "url": "https://domain.com",
  "email": "mongoexpuser@domain.com",
