  "repository": {
    "type": "aws, linode and g-drive",
    "url": "https://domain.com"
  },
  "homepage": "https://domain.com",
  "napi": {
    "name": "callRust",
    "triples": {
      "defaults": true,
      "addition": [
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "aarch64-unknown-linux-gnu",
        "x86_64-unknown-illumos",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl"
      ]
    }
